{
  "expo": {
    "name": "C-ode to you",
    "slug": "code2u",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.code2u.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.code2u.app",
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.681908599014-r6hdbp82iemnj3e7ogqj374mjd5l5nul",
          "androidClientId": "681908599014-mav3ri4iqqa6ofhjlovpld3sc8j0fj1i.apps.googleusercontent.com"
        }
      ]
    ],
    "scheme": "code2u",
    "extra": {
      "googleOAuth": {
        "webClientId": "681908599014-r6hdbp82iemnj3e7ogqj374mjd5l5nul.apps.googleusercontent.com",
        "androidClientId": "681908599014-mav3ri4iqqa6ofhjlovpld3sc8j0fj1i.apps.googleusercontent.com"
      },
      "eas": {
        "projectId": "cd1f1b7b-c1e8-4d2d-b4f5-b78a11495c73"
      }
    }
  }
}
